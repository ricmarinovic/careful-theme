{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Careful",
  "author": "Ricardo Marinovic",
  "themes": [
    {
      "name": "Careful Dark",
      "appearance": "dark",
      "style": {
        "background": "#101010",
        "editor.background": "#101010",
        "panel.background": "#101010",
        "status_bar.background": "#101010",
        "tab_bar.background": "#101010",
        "title_bar.background": "#101010",
        "toolbar.background": "#101010",
        "editor.gutter.background": "#101010",
        "surface.background": "#101010",
        "terminal.background": "#101010",
        "elevated_surface.background": "#222222",
        "drop_target.background": "#303030cc",

        "border": "#505050",
        "border.variant": "#303030",
        "border.focused": "#603a9c",
        "border.selected": "#ff0000",
        "border.transparent": "#ff0000",
        "border.disabled": "#ff0000",

        "element.active": "#505050",
        "element.background": "#202020",
        "element.disabled": "#ff0000",
        "element.hover": "#603a9c50",
        "element.selected": "#603a9c90",

        "ghost_element.active": "#505050",
        "ghost_element.background": "#202020",
        "ghost_element.disabled": "#ff0000",
        "ghost_element.hover": "#603a9c50",
        "ghost_element.selected": "#603a9c90",

        "pane.focused_border": "#603a9c",
        "panel.focused_border": "#603a9c",
        "tab.active_background": "#303030",
        "tab.inactive_background": "#101010",

        "text": "#e0e0e0",
        "text.accent": "#a26eea",
        "text.muted": "#c0c0c0",
        "text.disabled": "#909090",
        "text.placeholder": "#606060",
        "link_text.hover": "#a26eea",

        "editor.foreground": "#c0c0c0",
        "editor.line_number": "#505050",
        "editor.active_line.background": "#303030d4",
        "editor.active_line_number": "#ffffff",
        "editor.invisible": "#303030",
        "editor.subheader.background": "#603a9c99",
        "editor.highlighted_line.background": "#303030",
        "editor.document_highlight.bracket_background": "#60606040",
        "editor.document_highlight.read_background": "#303030",
        "editor.document_highlight.write_background": "#404040",
        "editor.wrap_guide": "#303030",
        "editor.active_wrap_guide": null,

        "search.match_background": "#603a9c70",

        "scrollbar_thumb.background": "#33333380",
        "scrollbar.thumb.hover_background": "#333333cc",
        "scrollbar.thumb.border": "#303030",
        "scrollbar.track.border": "#303030",
        "scrollbar.track.background": "#101010",

        "info": "#83afe5",
        "info.background": "#303030",
        "info.border": "#603a9ccc",
        "success": "#83ce4d",
        "success.background": "#83ce4d90",
        "success.border": null,
        "warning": "#d8a13b",
        "warning.background": "#d8a13b90",
        "warning.border": null,
        "error": "#ff615a",
        "error.background": null,
        "error.border": null,
        "hidden": "#606060",
        "hidden.background": null,
        "hidden.border": null,
        "ignored": "#707070",
        "ignored.background": null,
        "ignored.border": null,
        "hint": "#707070",
        "hint.background": null,
        "hint.border": null,
        "created": "#a6d189",
        "created.background": null,
        "created.border": null,
        "modified": "#ebcb8b",
        "modified.background": null,
        "modified.border": null,
        "deleted": "#ff615a",
        "deleted.background": null,
        "deleted.border": null,

        "players": [
          {
            "cursor": "#a26eea",
            "background": "#ff0000",
            "selection": "#606060aa"
          }
        ],

        "syntax": {
          "variable": { "color": "#c0c0c0" },
          "punctuation": { "color": "#c0c0c0" },
          "delimiter": { "color": "#c0c0c0" },

          "comment": { "color": "#666666" },

          "keyword": { "color": "#ffffff" },
          "keyword.function": { "color": "#ffffff" },
          "keyword.type": { "color": "#ffffff" },

          "attribute": { "color": "#8b98ac" },
          "property": { "color": "#8b98ac" },
          "string.special.symbol": { "color": "#8b98ac" },
          "symbol": { "color": "#8b98ac" },
          "enum": { "color": "#8b98ac" },
          "comment.doc": { "color": "#8b98ac" },
          "comment.documentation": { "color": "#8b98ac" },
          "type.builtin": { "color": "#8b98ac" },
          "title": { "color": "#8b98ac", "font_weight": 900 },

          "function.builtin": { "color": "#867edf" },
          "constant": { "color": "#867edf" },

          "function": { "color": "#8ea9d1" },

          "module": { "color": "#c48ed1" },
          "tag": { "color": "#c48ed1" },
          "type": { "color": "#c48ed1" },

          "constant.builtin": { "color": "#df8c8c" },
          "punctuation.special": { "color": "#df8c8c" },
          "string.escape": { "color": "#df8c8c" },
          "keyword.return": { "color": "#df8c8c" },
          "keyword.exception": { "color": "#df8c8c" },

          "number": { "color": "#ebcb8b" },

          "operator": { "color": "#dba849" },

          "string": { "color": "#71945c" },

          "character": { "color": "#90c275" },
          "string.special": { "color": "#90c275" },

          "emphasis.strong": { "font_weight": 900 }
        }
      }
    }
  ]
}
